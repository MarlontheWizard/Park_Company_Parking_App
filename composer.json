    "type": "project",
    "require": {
      "google/apiclient": "^2.0",  
      "vlucas/phpdotenv": "^5.0"   
    },

    "autoload": {
      "psr-4": {
        "App\\Backend\\": "src/backend/",
        "App\\Lib\\": "src/lib/"
