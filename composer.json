{
    "name": "software_engineering/park_app",
    "description": "Web application for users to find parking and pay online. Login uses Google authentication.",
    "type": "project",
    "require": {
      "google/apiclient": "^2.0",  
      "google/apiclient-services": "^0.381.0",
      "vlucas/phpdotenv": "^5.0",
      "stripe/stripe-php": "^10.0"
    },
    "autoload": {
      "psr-4": {
        "App\\Backend\\": "Web_Application/src/backend/",
        "App\\Frontend\\": "Web_Application/src/frontend/"
      }
    },

    "require-dev": {
      "phpunit/phpunit": "^9.5"  
    },
    
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "process-timeout": 3600
    }
}
